{
  "source": {
    "include": [
      "src/core/",
      "src/app/"
    ],
    "includePattern": "\\.(vue|js)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-vuejs"
  ],
  "opts": {
    "destination": "./dist/docs/",
    "recurse": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  }
}