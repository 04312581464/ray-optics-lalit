{
  "version": 5,
  "modules": {
    "RayRelay": {
      "params": [
        "f=-200:1:200:40",
        "d=1:1:500:80",
        "N=1:1:100:20"
      ],
      "objs": [
        {
          "for": "i=0:1:N-1",
          "type": "IdealLens",
          "p1": {
            "x": "`x_1+i*d`",
            "y": "`y_1`"
          },
          "p2": {
            "x": "`x_2+i*d`",
            "y": "`y_2`"
          },
          "focalLength": "`f`"
        }
      ]
    }
  }
}